       }\n        }\n    };\n})();\n\n$.exports = Zlib;\n"},{"name":"腾X","path":"danmu#腾X","rule":"js:\nfunction search(name) {\n    baseData = searchQQ(name);\n    let d = [];\n    let tempD = [];\n    // 普通结果\n    var baseList = baseData.normalList.itemList;\n    //  系列结果\n    var SerieList = baseData.areaBoxList;\n    try {\n        baseList.forEach(rs => {\n            if (rs.videoInfo != null) {\n                videoInfo = rs.videoInfo;\n                playSites = videoInfo.playSites[0];\n                tempD.push({\n                    title: videoInfo.title + \"⭐\" + videoInfo.typeName + '-' + videoInfo.year+'-来源:' + playSites.enName,\n                    url: playSites.episodeInfoList[0].url,\n                    影片名称:videoInfo.title\n                });\n            }\n        });\n    } catch (e) {\n        // 系列结果\n        log('系列结果');\n        SerieList.forEach(item => {\n            if (!item.boxShowName.match(/短视频/)) {\n                item.itemList.forEach(rs => {\n                    if (rs.videoInfo != null) {\n                        videoInfo = rs.videoInfo;\n                        playSites = videoInfo.playSites[0];\n                        tempD.push({\n                            title: videoInfo.title + \"⭐\" + videoInfo.typeName + '-' + videoInfo.year+'-来源:' + playSites.enName,\n                            url: playSites.episodeInfoList[0].url,\n                            影片名称:videoInfo.title\n                        });\n                    }\n                });\n            }\n        });\n\n    }\n    // 重组数据结构\n    d = tempD.map((it) => {\n        return {\n            title: it.title,\n            url: $().lazyRule((indexLink,影片名称) => {\n                if (indexLink.match(/cover/)) {\n                    // 手机搜索结果链接\n                    cid = indexLink.split('/')[5].split('.html')[0];\n                    if (indexLink.split('=')[1]) {\n                        vid = indexLink.split('=')[1];\n                        indexLink = 'ht