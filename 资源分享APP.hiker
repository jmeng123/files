tamp * 1000);\n\n    // 格式化日期部分\n    const year = date.getFullYear();\n    const month = ('0' + (date.getMonth() + 1)).slice(-2); // 月份从0开始\n    const day = ('0' + date.getDate()).slice(-2);\n\n    // 格式化时间部分\n    const hours = ('0' + date.getHours()).slice(-2);\n    const minutes = ('0' + date.getMinutes()).slice(-2);\n    const seconds = ('0' + date.getSeconds()).slice(-2);\n\n    // 返回格式化的日期和时间\n    return `${year}-${month}-${day} ${hours}:${minutes}:${seconds}`;\n}\n\nvar timestamp = new Date().getTime() / 1000; //log(timestamp)\nvar t10 = timestamp.toString().split('.')[0]; //log(t10)\nvar t13 = t10 + '000'; //log(t13)\nvar shijian = timestampToDate(t13); //log(shijian)\n\nif(getItem(\"zyfx\") == 'undefined'){\nif(getMyVar(\"zyfxyx\") != ''){\n    log(getMyVar(\"zyfxyx\"))\n//发送获取验证码请求\nvar htmlyzm = fetch('https://shareapi.jkshare.top/v2/sendVerification',{\n    headers: {\n        'key': t13+':4ea024f6ce3ea4ff:sendVerification',\n        'Content-Type': 'application/x-www-form-urlencoded',\n    },\n    body: 'email='+Encrypt(getMyVar('zyfxyx')),\n    method: 'POST'\n}); //log(htmlyzm)\ntoast(JSON.parse(Decrypt(htmlyzm)).msg)\n}\nif(getMyVar(\"zyfxzc\") != ''){\n    log(getMyVar(\"zyfxzc\"))\n//发送注册请求\nvar htmlzc = fetch('https://shareapi.jkshare.top/v2/register',{\n    headers: {\n        'key': t13+':4ea024f6ce3ea4ff:register',\n        'Content-Type': 'application/x-www-form-urlencoded',\n    },\n    body: 'json='+Encrypt(JSON.stringify({\"email\":getMyVar(\"zyfxzc\").split('#')[2],\"emailpush\":0,\"groupcode\":0,\"juanzeng\":0,\"password\":getMyVar(\"zyfxzc\").split('#')[1],\"qiandao\":0,\"registerandroidid\":\"4ea024f6ce3ea4ff\",\"username\":getMyVar(\"zyfxzc\").split('#')[0]}))+'&code='+getMyVar(\"zyfxzc\").split('#')[3],\n    method: 'POST'\n}); //log(htmlyzc)\ntoast(JSON.parse(Decrypt(htmlzc)).msg)\n}\n}\n\nif(getItem(\"zyfx\") != 'undefined'){\n//let time1 = '0'\nvar time = new Date().getDate().toString();\nif(getItem('time',time) != time||getItem('to