toString().padStart(2, '0'); // 确保日期是两位数\\n        // 组合年月日，并添加到数组中\\n        let formattedDate = year + '-' + month + '-' + day;\\n        dates.push(formattedDate); // 将日期添加到数组中\\n    }\\n    return dates; // 返回日期数组\\n}\\n\\n//authentication加密\\nfunction Encrypt(word) {\\n    let key = CryptoJS.enc.Utf8.parse('ziISjqkXPsGUMRNGyWigxDGtJbfTdcGv')\\n    let iv = CryptoJS.enc.Utf8.parse('WonrnVkxeIxDcFbv')\\n    let encrypt = CryptoJS.AES.encrypt(word, key, {\\n        iv: iv,\\n        mode: CryptoJS.mode.CBC,\\n        padding: CryptoJS.pad.Pkcs7,\\n    });\\n    let encryptedStr = encrypt.toString(CryptoJS.enc.base64);\\n    return encryptedStr.toString();\\n}\\nvar timestamp = new Date().getTime() / 1000; //log(timestamp)\\nvar t = timestamp.toString().split('.')[0]; //log(t)\\nvar t13 = t + '000'\\nvar authentication = Encrypt(base64Encode('3.0.0.3-'+t13+'-Android-1.0.1.7-f349051cdcfc4d91b5d66d561d27fd5a')); //log(authentication)\\n\\n//请求函数\\nfunction gethtml(url,method){\\nvar html = fetch(url,{\\n    headers: {\\n        'ts': t13,\\n        'authentication': authentication\\n  },\\n    method: `${method}`\\n}); //log(html)\\nvar rsakey = ''; //log(rsakey)\\nvar data = html.split('.')[0]; //log(data)\\nvar data2 = html.split('.')[1]; //log(data2)\\nvar key = rsaDecrypt(data,rsakey); //log(key)\\nvar iv = reverseIV(key); //log(iv)\\nvar html2 = Decrypt(data2,key,iv); //log(html2)\\nreturn html2\\n}\\n\\n//弹幕函数 by cy\\nfunction fetchDanmuData(vid, play, part) {\\n    let start_time_point = 0;\\n    let end_time_point = 60000;\\n    let limit = 10;\\n    let page = 1;\\n    \\n    // 在循环外声明并初始化 `danmustr`\\n    let danmustr = `<?xml version=\\\"1.0\\\" encoding=\\\"UTF-8\\\"?>\\\\n<i>\\\\n`;\\n\\n    while (true) {\\n        // 构建请求URL\\n        let url = `http://124.71.155.49:8091/app/danmu?vid=${vid}&play=${play}&part=${part}&start_time_point=${start_time_point}&end_time_point=${end_time_point}&limit=${limit}&page=${page}`; //log(url);\\n\\n        // 发送请求获取数据\\n        var html = fetch(url, {\\n            headers: {\\n                'ts': t13,\\n                'authentication': authentication\\n                },\\n            method: 'GET'\\n        }); //log(html)\\n\\n        var items = JSON.parse(html).data.items; //log(items.length)\\n\\n        for (let i = 0; i < items.length; i++) {\\n            var time_point = items[i].time_point / 1000; //log(time_point)\\n            var contents = JSON.parse(items[i].content); //log(contents)\\n            var color = contents.color; //log(color)\\n            var content = contents.content; //log(content)\\n            if (content.indexOf('<') != -1 || content.indexOf('>') != -1 || content.indexOf('&') != -1 || content.indexOf('\\\\u0000') != -1 || content.indexOf('\\\\b') != -1) {\\n                continue;\\n            }\\n            // 只将实际弹幕数据追加到 `danmustr`\\n            //时间，顶部弹幕，字体大小，颜色，0，弹幕内容\\n            let tempdata = `<d p=\\\"${time_point},1,25,${color},0\\\">${content}</d>\\\\n`; //log(tempdata)\\n            danmustr += tempdata;\\n        }\\n\\n        // 检查返回的数据是否为空，如果为空则停止循环\\n        if (items.length === 0) {\\n            break;\\n        }\\n\\n        // 更新时间点\\n        start_time_point = end_time_point;\\n        end_time_point += 60000; // 下一个时间段\\n    }\\n\\n    // 在循环结束后关闭 `danmustr` 的 XML 标签\\n    danmustr += `</i>`; //log(danmustr);\\n\\n    // 将最终结果写入文件\\n    writeFile('hiker://files/cache/danmu.xml', danmustr);\\n}\\n\\n//二级美化  by季风\\nfunction colorCode(d) {\\n    var str = Array.from(d.toString().replace('#', ''));\\n    if (str.length != 6) {\\n        return false;\\n    } else {\\n        for (var k in str) {\\n            if (!((str[k] >= '0' && str[k] <= '9') || (str[k] >= 'a' && str[k] <= 'f') || (str[k] >= 'A' && str[k] <= 'F'))) {\\n                return false;\\n            }\\n        }\\n        return true;\\n    }\\n}    \\nfunction color(d, c) {if (colorCode(d) && !colorCode(c)) {\\n        var tmp = d;\\n        d = c;\\n        c = tmp;\\n    }\\n    return '‘‘’’<font color=#' + (c || '000000') + '>' + d + '</font>';\\n}\\nfunction snum(index,title){\\n    if (index < 10) {\\n                    var num = String.fromCharCode(parseInt(index)  +0+10101);\\n                } else if (index < 20) {\\n                    var num = String.fromCharCode(parseInt(index) + 0 + 9440);\\n                } else if (index < 35) {\\n                    var num = String.fromCharCode(parseInt(index) + 0 + 12860);\\n                } else if (index < 50) {\\n                    var num = String.fromCharCode(parseInt(index) + 0 + 12941);\\n                } else {\\n                    var num = parseInt(index) + 0 + '.';\\n                }\\n     var Title = (index == 0 ? color(num, 'FF2244') : index == 1 ? color(num, 'FF6633') : index == 2 ? color(num, 'FFBB33') : color(num, '555555')) + color(' ' + title, 555555)\\n     return Title\\n}    \\nfunction combinPath(dire, path) {\\n    let Proto = '';\\n    if (dire.includes(\\\"//\\\")) {\\n        [Proto, _, dire] = dire.split(/(\\\\/{2})/);\\n        Proto += _;\\n    }\\n    const Paths = java.nio.file.Paths;\\n    dire = dire + path;\\n    var p = Paths.get(dire).toAbsolutePath().normalize();\\n    return Proto + String(p);\\n}    \\nfunction substr(str, maxLength) {\\n  let len = 0;\\n  for (let i = 0; i < str.length; i++) {\\n    if (str.charCodeAt(i) > 255) {\\n      len += 2;\\n    } else {\\n      len++;\\n    }\\n    if (len > maxLength) {\\n      return str.slice(0, i) + '...';\\n    }\\n  }\\n  return str;\\n}\\nfunction setDesc(arr, desc, num) {\\n  //log(desc)\\n  if (desc == undefined) {\\n    return;\\n  }\\n  desc = desc.constructor == Array ? desc.join('<br>') : desc;\\n  if (desc.replace(/(<br>|\\\\s+|<\\\\/?p>|&nbsp;)/g, '').length == 0) {\\n    return;\\n  }\\n\\n  const mark = 'desc';\\n  num = typeof (num) == 'undefined' ? 100 : num\\n  desc = desc.startsWith('　　') ? desc : '　　' + desc;\\n  desc = desc.replace(/'/g, \\\"&#39